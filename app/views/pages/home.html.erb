<div class="jumbotron">
	<h1>Whatup, world!</h1>
	<% unless user_signed_in? %>
	<p>
		<%= link_to "Log In", new_user_session_path , class: "btn btn-default btn-med" %>
		<%= link_to "Sign Up", new_user_registration_path , class: "btn btn-primary btn-med" %>
	</p>

	<% else %>
		<%= link_to "Log Out", destroy_user_session_path , method: :delete , class: "btn btn-primary btn-med" %>
	
	<% end %>


</div>

<p>
	Also, right now it is <%= Time.now %>
</p>